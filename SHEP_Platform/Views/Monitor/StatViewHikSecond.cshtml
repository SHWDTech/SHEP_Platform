
@{
    ViewBag.Title = "工程实时状况查看";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div style="float: right;">
    <object classid="clsid:41353907-A6CF-4BDF-9AE0-EB06F8C48414"
            width="800" height="600" id="StatMonitor" style="z-index: -1;"></object>
</div>
<div style="margin-right: 25px;">
    <div class="form-inline">
        <div style="margin-bottom: 30px;">
            <input type="button" class="form-control camerabtn"/>
            <input type="button" onmousedown="Up()" onmouseup="Stop()" value="上" class="form-control camerabtn">
            <input type="button" class="form-control camerabtn" />
        </div>
        <div style="margin-bottom: 30px;">
            <input type="button" onmousedown="Left()" onmouseup="Stop()" value="左" class="form-control camerabtn">
            <input type="button" onmousedown="StartMonitor()" value="开始" class="form-control camerabtn">
            <input type="button" onmousedown="Right()" onmouseup="Stop()" value="右" class="form-control camerabtn">
        </div>
        <div style="margin-bottom: 30px;">
            <input type="button" onmousedown="ZoomIn()" onmouseup="Stop()" value="近景" class="form-control camerabtn">
            <input type="button" onmousedown="Down()" onmouseup="Stop()" value="下" class="form-control camerabtn">
            <input type="button" onmousedown="ZoomOut" onmouseup="Stop()" value="远景" class="form-control camerabtn">
        </div>
</div>
</div>

@section scripts {
    <script src="/Scripts/Page/StatViewHikSecond.js"></script>
}
